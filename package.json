{
  "name": "gas-module",
  "version": "2.1.2",
  "description": "Import local google appscript files written with .js or .ts extension for testing",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "testsum": "jest 2>&1 | grep '‚óè' | cat -n"
  },
  "jest": {
    "rootDir": "./",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "__tests__/mocks/*"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/*.{js,ts}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  },
  "keywords": [
    "gas",
    "google app script",
    "module",
    "gas module"
  ],
  "author": "Naveen K R",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/me-nkr/gas-module.git"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3"
  }
}
